package Project2;

public class Q6 {
	
	public static void isEven(int n) {
		int numDivideByTwo = n/2;
		int numMultiplyByTwo = numDivideByTwo*2;
		
		if (n == numDivideByTwo) {
			System.out.println(n + " is an even number");
		} else {
			System.out.println(n + " is an odd number");
		}
		
		
	}

}
